//This actor will serve to see if the voxels imported correctly
//Rewrite as neccesary

Actor VoxelTest 15222
{
	Height 72
	Radius 2
	XScale 2.25
	YScale 1.7
	+NOCLIP
	+NOGRAVITY
	+CEILINGHUGGER
	//$Category "Zombies Ate My Neighbors"
	States
	{
		Spawn:
		SARC A 10
		SARC A 10
		Loop
	}
}

Actor SpriteTest 15223
{
	Health 2000
	Speed 20
	Height 64
	Radius 16
	XScale 1.75
	YScale 1.75
	//+NOCLIP
	//+NOGRAVITY
	+NOBLOOD
	Monster
	//$Category "Zombies Ate My Neighbors"
	States
	{
		Spawn:
		TNT1 A 1 A_Look
		//ANTT AABBCCDD 4 A_Look
		Loop
		See:
		ANTW AAABBBCCCDDD 3 A_Chase
		ANTW AAABBBCCCDDD 3 A_Chase
		ANTW AAABBBCCCDDD 3 A_Chase
		ANTW AAABBBCCCDDD 3 A_Chase
		ANTD ABCDE 5
		TNT1 A 20

		ANTS ABC 10
		Loop

	}
}
