			"####" YZYZYZYZ 5
			"####" YZ 5
			Goto Death
		//Longer variant for Pooldude Survivor
		RedGooAnimation_PoolDude:
			"####" YZYZYZYZ 5
			"####" YZYZYZYZ 5
			"####" YZYZYZYZ 5
			"####" YZYZYZYZ 5
			"####" YZYZYZYZ 5
			"####" YZYZYZYZ 5
			"####" YZYZYZYZ 5
			"####" YZYZYZYZ 5
