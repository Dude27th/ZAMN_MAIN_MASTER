Script "TestHelp" (void)
{
	Log(s: StrParam(s: "Pukename:", s: "\n--------------------", s: "\n+ TestMonsterLimit", s: "\n+ TestPitch", s: "\n+ TestCamera", s: "\n+ TestSpeed", s: "\n+ TestInvulnerable", s: "\n\n+ ReservedTags", s: "\n+ MusicInfo"));
}